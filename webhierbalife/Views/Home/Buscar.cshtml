@model List<webhierbalife.Models.Producto>
<body>
  

    <div id="wrapper" class="container">

        <section class="main-content">
            <div class="row">
                <div class="span12">
                    <div class="row">
                        <div class="span12">
                            <div class="container">
                                <div class="push-up">
                                    <div class="row">
                                        <div id="myCarousel" class="myCarousel carousel slide">
                                            <div class="carousel-inner">
                                                <div class="active item">
                                                  
                                               @foreach (var item in Model)
                                               {
                                                   var imagen = @item.ProductoImagen.FirstOrDefault();
                                                   string img = imagen == null ? "no_disponible.jpg" : imagen.Imagen;

                                                   <ul class="thumbnails">
                                                        <li class="span3">
                                                            <div class="product-box">
                                                                <span class="sale_tag"></span>
                                                                <p><a href="product_detail.html"><img src="~/themes/images/@img" alt="" /></a></p>
                                                                <div class="img-overlay">
                                                                    <a onclick="AgregarCarrito(@item.ProductoId,'@item.Denominacion','@img',@item.Precio )"class="btn buy btn-danger">al carrito</a>
                                                                </div>
                                                                <a href="" class="title">@item.Descripcion</a><br />
                                                                
                                                                <a href="" class="category">Commodo consequat</a>
                                                                <p class="price">S/@item.Precio </p>
                                                            </div>
                                                        </li>
                                                    </ul>
                                               }
                                                </div>
                                            
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                        </div>
                    </div>
                </div>
        </section>



    </div>
    <script src="~/themes/js/common.js"></script>
    <script src="~/themes/js/jquery.flexslider-min.js"></script>
    <script type="text/javascript">
			$(function() {
				$(document).ready(function() {
					$('.flexslider').flexslider({
						animation: "fade",
						slideshowSpeed: 4000,
						animationSpeed: 600,
						controlNav: false,
						directionNav: true,
						controlsContainer: ".flex-container" // the container that holds the flexslider
					});
				});
			});
    </script>
</body>
